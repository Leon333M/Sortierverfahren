{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Starte Sortierverfahren",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/Release/Sortierverfahren.exe",
      "args": [
        "1200",
        "i",
        "z",
        "i",
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "console": "externalTerminal"
    },
    {
      "name": "Starte Sortierverfahren Debug",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/Debug/Sortierverfahren.exe",
      "args": [
        "1200",
        "g",
        "z",
        "s",
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "console": "externalTerminal"
    }
  ]
}